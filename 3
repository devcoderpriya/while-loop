// Write a program to find all numbers divisible by 3 between two given numbers
#include<stdio.h>
int main()
{
int a;
printf("enter the number:");
scanf("%d",&a);

int b;
printf("enter the number:");
scanf("%d",&b);

int i=a;
while(i<=b)
{
    if(i%3==0){
     printf("%d\n",i);

}

i=i+1;
}
return 0;
}

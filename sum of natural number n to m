#include<stdio.h>
    int main()
    {
        int a;
        printf("enter the number:");
        scanf("%d",&a);
          
        int b;
        printf("enter the number:");
        scanf("%d",&b);
        int sum=0;
        while(a<=b)
        {
            sum=sum+a;
            a=a+1;
          
        }
       
    printf("%d",sum);
    return 0;
}
